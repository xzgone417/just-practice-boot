<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.exp.ucmp.inventory.dao.PsiCarFileDao">

    <select id="selectSaleRecordFile" resultType="com.exp.ucmp.file.dto.FileReturnDto">
        SELECT
            file_id,
            thumbnail
        FROM
            t_psi_car_sale_record_file
        WHERE
            sale_record_id = #{saleRecordId}
    </select>

</mapper>